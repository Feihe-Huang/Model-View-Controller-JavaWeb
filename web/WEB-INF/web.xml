<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>SignupServlet</servlet-name>
        <servlet-class>example.SignupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignupServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>example.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/log</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SignupServlet1</servlet-name>
        <servlet-class>example.SignupServlet 1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignupServlet1</servlet-name>
        <url-pattern>/hello1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddExhibition</servlet-name>
        <servlet-class>example.AddExhibition</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddExhibition</servlet-name>
        <url-pattern>/adde</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddCustomer</servlet-name>
        <servlet-class>example.AddCustomer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddCustomer</servlet-name>
        <url-pattern>/addc</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddGallery</servlet-name>
        <servlet-class>example.AddGallery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddGallery</servlet-name>
        <url-pattern>/addg</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadPainting</servlet-name>
        <servlet-class>example.UploadPainting</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadPainting</servlet-name>
        <url-pattern>/addp</url-pattern>
    </servlet-mapping>


</web-app>